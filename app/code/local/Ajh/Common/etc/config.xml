<?xml version="1.0"?>
<config>

	<modules>
		<Ajh_Common>
			<version>0.0.1</version>
		</Ajh_Common>
	</modules>

	<frontend>
		<routers>
			<common>
				<use>standard</use>
				<args>
					<module>Ajh_Common</module>
					<frontName>common</frontName>
