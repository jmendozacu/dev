    var contactForm = new VarienForm('contactForm', true);
    document.observe('dom:loaded', function () {
        if ($('contact_form')) {
            $('contact_form').captcha.refresh($('catpcha-reload'));
            $('captcha-reload').observe('click', function () {
                $('contact_form').captcha.refresh($('catpcha-reload'));
            });
        }
    });
    Validation.addAllThese(
            [
                ['validate-entry', '<?php echo $this->__('This is a required field.') ?>', function (v) {
                        if (v.toLowerCase() == 'subject' || v.toLowerCase() == 'name' || v.toLowerCase() == 'email' || v.toLowerCase() == 'confirm email' || v.toLowerCase() == 'telephone' || v.toLowerCase() == 'your message here') {
                            return false;
                        }
                        return true;
                    }],
                ['validate-confirm-email', 'Email addresses do not match.', function (v) {
                        var conf = $('confirmation_email_address') ? $('confirmation_email_address') : $$('.validate-confirm-email')[0];
                        var pass = false;
                        if ($('email')) {
                            pass = $('email');
                        }

                        return (pass.value == conf.value);
                    }],
            ]
            );
    //]]>
</script>
