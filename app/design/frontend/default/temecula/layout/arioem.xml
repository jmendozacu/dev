<?xml version="1.0"?>
<layout version="0.1.0">

	<default>
		<reference name="head">
			<action method="addItem">
				<type>skin_css</type>
				<name>arioem/arioem.css</name>
			</action>
		</reference>
	</default>


	<arioem_parts_index>
